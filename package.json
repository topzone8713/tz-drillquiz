{
  "name": "drillquiz-frontend",
  "version": "1.0.0",
  "description": "DrillQuiz Frontend",
  "main": "index.js",
  "scripts": {
    "serve": "FRONTEND_PORT=8080 vue-cli-service serve",
    "serve:dev": "ENVIRONMENT=development VUE_APP_ENVIRONMENT=development FRONTEND_PORT=8080 vue-cli-service serve",
    "serve:prod": "ENVIRONMENT=production VUE_APP_ENVIRONMENT=production vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:dev": "ENVIRONMENT=development VUE_APP_ENVIRONMENT=development vue-cli-service build",
    "build:prod": "ENVIRONMENT=production VUE_APP_ENVIRONMENT=production VUE_APP_API_HOST=us.drillquiz.com VUE_APP_API_PORT=443 VUE_APP_API_PROTOCOL=https vue-cli-service build",
    "build:prod-dev": "ENVIRONMENT=production VUE_APP_ENVIRONMENT=production VUE_APP_API_HOST=us-dev.drillquiz.com VUE_APP_API_PORT=443 VUE_APP_API_PROTOCOL=https vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "i18n:extract": "python scripts/translation-extractor.py --extract-only",
    "i18n:compile": "python scripts/translation-extractor.py --compile-only",
    "i18n:sync": "python scripts/translation-extractor.py"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.0.0",
    "@popperjs/core": "^2.11.8",
    "axios": "^1.6.0",
    "bootstrap": "^5.3.2",
    "bootstrap-vue": "^2.22.0",
    "chart.js": "^4.5.0",
    "file-saver": "^2.0.5",
    "openai": "^5.19.1",
    "recordrtc": "^5.6.2",
    "vue": "^2.6.14",
    "vue-i18n": "^8.28.2",
    "vue-meta": "^2.4.0",
    "vue-router": "^3.5.4",
    "vuedraggable": "^2.24.3",
    "webrtc-adapter": "^9.0.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.0",
    "@playwright/test": "^1.57.0",
    "@vue/cli-plugin-babel": "^5.0.9",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "is-mergeable-object": "^1.1.1",
    "patch-package": "^8.0.1",
    "postinstall-postinstall": "^2.1.0",
    "vue-template-compiler": "^2.7.16"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
